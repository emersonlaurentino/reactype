{
  "extends": [
    "tslint-config-airbnb",
    "tslint-eslint-rules",
    "tslint-react"
  ],
  "rules": {
    "global-require": 0,
    "no-confusing-arrow": 0,
    "no-param-reassign": 0,
    "no-plusplus": 0,
    "no-underscore-dangle": 0,
    "one-var-declaration-per-line": 0,
    "one-var": 0
  }
}
